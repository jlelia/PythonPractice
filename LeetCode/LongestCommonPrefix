class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        lcp = min(strs, key=len)
        for s in strs:
            if s == lcp:
                continue
            while not s.startswith(lcp):
                lcp = lcp[:-1]
                if lcp == "":
                    return ""
        return lcp
